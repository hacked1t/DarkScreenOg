<?xml version="1.0" encoding="utf-8"?>

<data-extraction-rules>
    <cloud-backup>
        <include domain="file" path="datastore/user.prefs.preferences.p" />
    </cloud-backup>

    <device-transfer>
        <include domain="file" path="datastore/user.prefs.preferences.pb"/>
        <exclude domain="cache" />
        <exclude domain="database" />
    </device-transfer>
</data-extraction-rules>